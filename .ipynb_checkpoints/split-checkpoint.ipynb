   "source": [
    "split(i, M) = div(i - 1, M) + 1, rem(i - 1, M) + 1\n",
    "\n",
    "function single_to_multi(m, I, J)\n",
    "    k, temp = split(m, I * J)\n",
    "    i, j = split(temp, I)\n",
    "    return k, i, (i, j)\n",
    "end"
   ]
